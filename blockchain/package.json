{
    "name": "asset-transfer-basic",
    "version": "1.0.0",
    "description": "Asset-Transfer-Basic contract implemented in JavaScript",
    "main": "index.js",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "test": "nyc mocha test --recursive",
        "lint": "eslint .",
        "start": "fabric-chaincode-node server --chaincode-address=0.0.0.0:9999 --chaincode-id=$CHAINCODE_ID"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-contract-api": "~2.5",
        "fabric-shim": "~2.5",
        "json-stringify-deterministic": "^1.0.0",
        "sort-keys-recursive": "^2.1.0"
    },
    "devDependencies": {
        "chai": "^4.5.0",
        "eslint": "^8.57.0",
        "mocha": "^10.8.2",
        "nyc": "^15.1.0",
        "sinon": "^18.0.1",
        "sinon-chai": "^3.7.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**",
            "index.js",
            ".eslintrc.js"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
